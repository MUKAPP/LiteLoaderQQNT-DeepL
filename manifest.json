{
    "manifest_version": 3,
    "type": "extension",
    "name": "DeepL",
    "slug": "deepl_plugin",
    "description": "将 DeepL 翻译接入你的 QQNT",
    "version": "1.0.5",
    "thumbnail": "./res/icon.webp",
    "author": {
        "name": "MUKAPP",
        "link": "https://github.com/MUKAPP"
    },
    "repository": {
        "repo": "MUKAPP/LiteLoaderQQNT-DeepL",
        "branch": "main",
        "use_release": {
            "tag": "latest",
            "name": "LiteLoaderQQNT-DeepL.zip"
        }
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}
